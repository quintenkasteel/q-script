{
  "name": "@quintenkasteel/q-script",
  "version": "0.1.2",
  "description": "utility functions",
  "main": "dist/index.js",
  "author": "Quinten Kasteel",
  "license": "ISC",
  "scripts": {
    "coverage": "jest --coverage",
    "build": "webpack --config webpack.config.js --mode production",
    "build.prod": "webpack --config webpack.config.js -p",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build.esnext": "npm run build:types && npm run build:js",
    "build.all": "npm run build.prod && npm run build.esnext",
    "test": "karma start karma.config.js",
    "dev": "webpack",
    "start": "webpack-dev-server --mode development --open"
  },
  "repository": "https://github.com/quintenkasteel/q-script",
  "keywords": [],
  "engines": {
    "node": ">=4.3.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jasmine": "2.8.7",
    "@types/node": "7.0.0",
    "awesome-typescript-loader": "5.2.0",
    "babel-loader": "^8.1.0",
    "eslint": "^5.16.0",
    "html-webpack-plugin": "^4.3.0",
    "jasmine-core": "3.1.0",
    "jest": "^24.9.0",
    "karma": "2.0.4",
    "karma-jasmine": "1.1.2",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-webpack": "3.0.0",
    "source-map-loader": "0.2.3",
    "tslint": "5.17.0",
    "tslint-loader": "3.6.0",
    "typescript": "^3.9.7",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4"
  }
}
