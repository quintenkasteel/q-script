{
  "name": "@quintenkasteel/q-script",
  "version": "0.1.15",
  "description": "utility functions",
  "type": "module",
  "main": "es/index.js",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "author": "Quinten Kasteel",
  "license": "ISC",
  "files": ["dist", "es"],
  "scripts": {
    "coverage": "jest --coverage",
    "build.prod": "webpack --config webpack.config.js -p",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build.esnext": "npm run build:types && npm run build:js",
    "test": "karma start karma.config.js",
    "build": "webpack --config webpack.config.js",
    "start": "live-server --open=demo",
    "dev": "webpack-dev-server --mode development --open",
    "build-es": "babel src --out-dir es",
    "build.all": "npm run build && npm run build-es"
  },
  "repository": "https://github.com/quintenkasteel/q-script",
  "keywords": [],
  "engines": {
    "node": ">=9.11.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jasmine": "2.8.7",
    "@types/node": "7.0.0",
    "awesome-typescript-loader": "5.2.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^5.16.0",
    "html-webpack-plugin": "^4.3.0",
    "jasmine-core": "3.1.0",
    "jest": "^24.9.0",
    "live-server": "^1.2.1",
    "source-map-loader": "0.2.3",
    "tslint": "5.17.0",
    "tslint-loader": "3.6.0",
    "typescript": "^3.9.7",
    "webpack": "^4.4.1",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.11"
  }
}
